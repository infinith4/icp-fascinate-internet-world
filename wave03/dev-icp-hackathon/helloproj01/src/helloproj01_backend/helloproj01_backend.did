type PasswordEntry = record {
    service_name : text;
    username : text;
    password : text;
    encrypted: text;
    iv: text;
    salt: text;
    notes : opt text;
};

service : {
    "greet": (text) -> (text) query;
    "get_passwords": () -> (vec PasswordEntry) query;
    "add_password": (PasswordEntry) -> (bool);
    "update_password": (nat64, PasswordEntry) -> (bool);
    "delete_password": (nat64) -> (bool);
}
